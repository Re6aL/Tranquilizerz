<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <ThingDef Class="AnestheticGunMod2.AnestheticBulletDef" ParentName="BaseBullet">
    <defName>Bullet_AnestheticGun</defName>
    <label>Anesthetic syringe</label>
    <projectile>
      <damageDef>Bullet</damageDef>
      <damageAmountBase>1</damageAmountBase>
      <stoppingPower>1.1</stoppingPower>
      <speed>40</speed>
    </projectile>
    <graphicData>
      <texPath>Things/Projectile/Bullet_big</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <!-- configuration for anesthetic gun bullet -->
    <thingClass>AnestheticGunMod2.AnestheticBullet</thingClass>
    <anestheticBulletPower>10</anestheticBulletPower> 
    <anestheticBulletExcludedes>
      <li><pawnKindDef>Mech_Scyther</pawnKindDef></li>
      <li><pawnKindDef>Mech_Centipede</pawnKindDef></li>
      <li><pawnKindDef>Mech_Lancer</pawnKindDef></li>
      <li><pawnKindDef>Mech_Pikeman</pawnKindDef></li>
    </anestheticBulletExcludedes>
  </ThingDef>
  
</Defs>
